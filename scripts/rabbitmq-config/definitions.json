{
    "rabbit_version": "3.8.9",
    "rabbitmq_version": "3.8.9",
    "product_name": "RabbitMQ",
    "product_version": "3.8.9",    
    "vhosts": [
      {
        "name": "/"
      }
    ],
    "users": [
        {
         "name": "guest",
         "password": "guest",         
          "tags": "administrator"
        }
      ],
    "permissions": [
      {
        "user": "guest",
        "vhost": "/",
        "configure": ".*",
        "write": ".*",
        "read": ".*"
      }
    ],
    "exchanges": [
      {"name": "dapr-long-running-jobs-exchange", "vhost": "/", "type": "fanout", "durable": true, "auto_delete": false, "internal": false, "arguments": {}},
      {"name": "system-logs-exchange", "vhost": "/", "type": "fanout", "durable": true, "auto_delete": false, "internal": false, "arguments": {}},
      {"name": "user-notifications-exchange", "vhost": "/", "type": "fanout", "durable": true, "auto_delete": false, "internal": false, "arguments": {}}
    ],
    "queues": [
      {
        "name": "long_running_jobs_queue",
        "vhost": "/",
        "durable": true,
        "auto_delete": false,
        "arguments": {
          "x-dead-letter-exchange": "dlx-long_running_jobs_queue"
        }
      },
      {
        "name": "dlx-long_running_jobs_queue",
        "vhost": "/",
        "durable": true,
        "auto_delete": false,
        "arguments": {}
      },
      {
        "name": "system_logs_queue",
        "vhost": "/",
        "durable": true,
        "auto_delete": false,
        "arguments": {}
      },
      {
        "name": "user_notifications_queue",
        "vhost": "/",
        "durable": true,
        "auto_delete": false,
        "arguments": {}
      }
    ],
    "bindings": [
      {
        "source": "dapr-long-running-jobs-exchange",
        "vhost": "/",
        "destination": "long_running_jobs_queue",
        "destination_type": "queue",
        "routing_key": "job.run",
        "arguments": {}
      },
      {
        "source": "system-logs-exchange",
        "vhost": "/",
        "destination": "system_logs_queue",
        "destination_type": "queue",
        "routing_key": "system.log",
        "arguments": {}
      },
      {
        "source": "user-notifications-exchange",
        "vhost": "/",
        "destination": "user_notifications_queue",
        "destination_type": "queue",
        "routing_key": "user.notify",
        "arguments": {}
      }
    ]
  }